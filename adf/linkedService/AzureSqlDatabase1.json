{
	"name": "AzureSqlDatabase1",
	"type": "Microsoft.DataFactory/factories/linkedservices",
	"properties": {
		"parameters": {
			"host": {
				"type": "string"
			},
			"db": {
				"type": "string"
			},
			"username": {
				"type": "string"
			},
			"password": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "AzureSqlDatabase",
		"typeProperties": {
			"connectionString": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=@{linkedService().host};Initial Catalog=@{linkedService().db};User ID=@{linkedService().username}",
			"password": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "iigkeyvault",
					"type": "LinkedServiceReference"
				},
				"secretName": "@{linkedService().password}"
			}
		}
	}
}